// candies problem

import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc=new Scanner (System.in);
        int n=10;
        int k=5;
        int num=sc.nextInt();
        
        if(num>=1 && num<=k){
            System.out.println("no of candies sold = "+num);
            System.out.println("no of candies available = "+(n-num));
        }
        else {
            System.out.println("invalid input");
            System.out.println("no of candies available = "+n);
            
        }
    }
}
